<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:common="http://schemas.radixware.org/commondef.xsd" xmlns:utils="http://schemas.radixware.org/utils.xsd" xmlns:editmask="http://schemas.radixware.org/editmask.xsd" xmlns:this="http://schemas.radixware.org/adsdef.xsd" xmlns:ui="http://schemas.radixware.org/ui.xsd" xmlns:xscml="http://schemas.radixware.org/xscml.xsd" xmlns:types="http://schemas.radixware.org/types.xsd" xmlns:algo="http://schemas.radixware.org/algo.xsd" xmlns:msdl="http://schemas.radixware.org/msdl.xsd" xmlns:web="http://schemas.radixware.org/web-ui.xsd" targetNamespace="http://schemas.radixware.org/adsdef.xsd" elementFormDefault="qualified" attributeFormDefault="unqualified">
    <xs:import namespace="http://schemas.radixware.org/xscml.xsd" schemaLocation="xscml.xsd"/>
    <xs:import namespace="http://schemas.radixware.org/editmask.xsd" schemaLocation="editmask.xsd"/>
    <xs:import namespace="http://schemas.radixware.org/commondef.xsd" schemaLocation="commondef.xsd"/>
    <xs:import namespace="http://schemas.radixware.org/ui.xsd" schemaLocation="ui.xsd"/>
    <xs:import namespace="http://schemas.radixware.org/types.xsd" schemaLocation="types.xsd"/>
    <xs:import namespace="http://schemas.radixware.org/algo.xsd" schemaLocation="algo.xsd"/>
    <xs:import namespace="http://schemas.radixware.org/msdl.xsd" schemaLocation="msdl.xsd"/>
    <xs:import namespace="http://schemas.radixware.org/utils.xsd" schemaLocation="utils.xsd"/>
    <xs:import namespace="http://schemas.radixware.org/web-ui.xsd" schemaLocation="web-ui.xsd"/>
    <xs:include schemaLocation="adsdef-min.xsd"/>
    <xs:annotation>
        <xs:documentation>--------- Abstract Base Types  ---------</xs:documentation>
    </xs:annotation>
    
    <xs:complexType name="MixableAdsDefinition">
        <xs:complexContent>
            <xs:extension base="common:DescribedDefinition">
                <xs:sequence>
                    <xs:element name="AccessRules" type="this:AccessRules" minOccurs="0"/>
                    <xs:element name="Domains">
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="Domain" minOccurs="0" maxOccurs="unbounded">
                                    <xs:complexType>
                                        <xs:attribute name="Path">
                                            <xs:simpleType>
                                                <xs:list itemType="types:Id"/>
                                            </xs:simpleType>
                                        </xs:attribute>
                                    </xs:complexType>
                                </xs:element>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
                <xs:attribute name="IdSrcEnum" type="types:Id" use="optional"/>
                <xs:attribute name="IdSrcEnumItem" type="types:Id" use="optional"/>
                <xs:attribute name="APIUsageKind" type="xs:int" use="optional" default="0"/>
                <xs:attribute name="MixTo" use="optional">
                    <xs:simpleType>
                        <xs:list itemType="types:Id"/>
                    </xs:simpleType>
                </xs:attribute>
                <xs:attribute name="UploadMode" use="required">
                    <xs:simpleType>
                        <xs:annotation>
                            <xs:appinfo source="http://schemas.radixware.org/types.xsd">
                                <types:class>org.radixware.kernel.common.enums.EDefinitionUploadMode</types:class>
                            </xs:appinfo>
                        </xs:annotation>
                        <xs:restriction base="types:IntEnum"/>
                    </xs:simpleType>
                </xs:attribute>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="DescribedAdsDefinition">
        <xs:complexContent>
            <xs:extension base="this:MixableAdsDefinition">
                
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="TitledAdsDefinition">
        <xs:complexContent>
            <xs:extension base="this:DescribedAdsDefinition">
                <xs:attribute name="TitleId" type="types:Id" use="optional"/>                
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="AccessRules">
        <xs:sequence>
            <xs:element name="Transparence" minOccurs="0">
                <xs:complexType>
                    <xs:attribute name="PublishedName" type="xs:string"/>
                    <xs:attribute name="Extendable" type="xs:boolean"/>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
        <xs:attribute name="Access" type="xs:int">
            <!--			<xs:simpleType>
                    <xs:annotation>
                            <xs:appinfo source="http://schemas.radixware.org/types.xsd">
                                    <types:class>org.radixware.kernel.common.enums.EAccess</types:class>
                            </xs:appinfo>
                    </xs:annotation>
                    <xs:restriction base="types:IntEnum"/>
            </xs:simpleType>-->
        </xs:attribute>
        <xs:attribute name="IsFinal" type="xs:boolean" use="optional" default="false"/>
        <xs:attribute name="IsAbstract" type="xs:boolean" use="optional" default="false"/>
        <xs:attribute name="IsStatic" type="xs:boolean" use="optional" default="false"/>
        <xs:attribute name="IsPublished" type="xs:boolean" use="optional" default="false"/>
        <xs:attribute name="IsDeprecated" type="xs:boolean" use="optional" default="false"/>
        <xs:attribute name="ExpirationRelease" type="xs:string" use="optional"/>
    </xs:complexType>
    <xs:annotation>
        <xs:documentation>--------- Common Types  ---------</xs:documentation>
    </xs:annotation>
    <xs:complexType name="SqmlCondition">
        <xs:sequence>
            <xs:element name="ConditionWhere" type="xscml:Sqml"/>
            <xs:element name="ConditionFrom" type="xscml:Sqml" minOccurs="0"/>
            <xs:element name="Prop2ValueCondition">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="Item" minOccurs="0" maxOccurs="unbounded">
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name="Value" type="xs:string" minOccurs="0"/>
                                </xs:sequence>
                                <xs:attribute name="PropId" type="types:Id"/>
                            </xs:complexType> 
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="ObjectTitleFormatDefinition">
        <xs:complexContent>
            <xs:extension base="common:Definition">
                <xs:sequence>
                    <xs:element name="TitleItem" minOccurs="0" maxOccurs="unbounded">
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="Pattern" type="xs:string" minOccurs="0"/>
                                <xs:element name="CustomNullFormat" minOccurs="0">
                                    <xs:complexType>
                                        <xs:sequence>
                                            <xs:element name="Pattern" type="xs:string" minOccurs="0"/>
                                        </xs:sequence>
                                        <xs:attribute name="PatternId" type="types:Id" use="optional"/>
                                    </xs:complexType>
                                </xs:element>
                            </xs:sequence>
                            <xs:attribute name="PropId" type="types:Id" use="required"/>
                            <xs:attribute name="PatternId" type="types:Id" use="optional"/>
                            <xs:attribute name="NullFormat">
                                <xs:simpleType>
                                    <xs:annotation>
                                        <xs:appinfo source="http://schemas.radixware.org/types.xsd">
                                            <types:class>org.radixware.kernel.common.enums.ETitleNullFormat</types:class>
                                        </xs:appinfo>
                                    </xs:annotation>
                                    <xs:restriction base="types:StrEnum"/>
                                </xs:simpleType>
                            </xs:attribute>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
                <xs:attribute name="NullTitleId" type="types:Id" use="optional"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="ExplorerItemDefinition">
        <xs:complexContent>
            <xs:extension base="this:AbstractExplorerItemDefinition">
                <xs:attribute name="InheritanceMask" type="xs:long" use="required"/>
                <xs:attribute name="ClassId" type="types:Id"/>
                <xs:attribute name="IsVisibleInTree" type="xs:boolean" default="true"/>
                <xs:attribute name="Restrictions" type="xs:long" default="0"/>
                <xs:attribute name="EnabledCommands">
                    <xs:simpleType>
                        <xs:list itemType="types:Id"/>
                    </xs:simpleType>
                </xs:attribute>
                <xs:attribute name="ClientEnvironment">
                    <xs:simpleType>
                        <xs:annotation>
                            <xs:appinfo source="http://schemas.radixware.org/types.xsd">
                                <types:class>org.radixware.kernel.common.enums.ERuntimeEnvironmentType</types:class>
                            </xs:appinfo>
                        </xs:annotation>
                        <xs:restriction base="types:StrEnum"/>
                    </xs:simpleType>
                </xs:attribute>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="SelectorExplorerItemDefinition">
        <xs:complexContent>
            <xs:extension base="this:ExplorerItemDefinition">
                <xs:sequence>
                    <xs:element name="Condition" type="this:SqmlCondition"/>
                </xs:sequence>
                <xs:attribute name="SelectorPresentationId" type="types:Id" use="required"/>
                <xs:attribute name="CreationClassCatalogId" type="types:Id"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="ExplorerItemExternalCondition">
        <xs:sequence>
            <xs:element name="Item" maxOccurs="unbounded">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="Element" maxOccurs="unbounded">
                            <xs:complexType>
                                <xs:attribute name="Visible" type="xs:boolean" use="optional"/>
                                <xs:attribute name="Id" type="types:Id"/>
                            </xs:complexType>
                        </xs:element>
                    </xs:sequence>
                    <xs:attribute name="TargetId" type="types:Id"/>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="ChildExplorerItems">
        <xs:sequence>
            <xs:element name="Order" type="this:ExplorerItemExternalCondition"/>
            <xs:element name="Visibility" type="this:ExplorerItemExternalCondition"/>
            <xs:element name="ExplicitlyInherited" minOccurs="0">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="Item" minOccurs="1" maxOccurs="unbounded">
                            <xs:complexType>
                                <xs:attribute name="Id" type="types:Id"/>
                            </xs:complexType>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="Item" minOccurs="0" maxOccurs="unbounded">
                <xs:complexType>
                    <xs:choice>
                        <xs:element name="ChildRef">
                            <xs:complexType>
                                <xs:complexContent>
                                    <xs:extension base="this:SelectorExplorerItemDefinition">
                                        <xs:attribute name="ChildReferenceId" type="types:Id"/>
                                    </xs:extension>
                                </xs:complexContent>
                            </xs:complexType>
                        </xs:element>
                        <xs:element name="ParentRef">
                            <xs:complexType>
                                <xs:complexContent>
                                    <xs:extension base="this:ExplorerItemDefinition">
                                        <xs:attribute name="ParentReferenceId" type="types:Id"/>
                                        <xs:attribute name="EditorPresentationIds">
                                            <xs:simpleType>
                                                <xs:list itemType="types:Id"/>
                                            </xs:simpleType>
                                        </xs:attribute>
                                    </xs:extension>
                                </xs:complexContent>
                            </xs:complexType>
                        </xs:element>
                        <xs:element name="Entity">
                            <xs:complexType>
                                <xs:complexContent>
                                    <xs:extension base="this:SelectorExplorerItemDefinition"/>
                                </xs:complexContent>
                            </xs:complexType>
                        </xs:element>
                        <xs:element name="Paragraph" type="this:ParagraphDefinition"/>
                        <xs:element name="ParagraphLink">
                            <xs:complexType>
                                <xs:complexContent>
                                    <xs:extension base="this:AbstractExplorerItemDefinition">
                                        <xs:attribute name="InheritanceMask" type="xs:long" use="required"/>
                                        <xs:attribute name="ParagraphId" type="types:Id" use="required"/>
                                    </xs:extension>
                                </xs:complexContent>
                            </xs:complexType>
                        </xs:element>
                    </xs:choice>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="ParagraphDefinition">
        <xs:complexContent>
            <xs:extension base="this:AbstractExplorerItemDefinition">
                <xs:sequence>
                    <xs:element name="ChildExplorerItems" type="this:ChildExplorerItems" minOccurs="0"/>
                    <xs:element name="Model" type="this:ClassDefinition"/>
                    <xs:element name="View" type="this:AbstractDialogDefinition" minOccurs="0"/>
                    <xs:element name="WebView" type="this:AbstractDialogDefinition" minOccurs="0"/>
                </xs:sequence>
                <xs:attribute name="UsedContextlessCommands">
                    <xs:simpleType>
                        <xs:list itemType="types:Id"/>
                    </xs:simpleType>
                </xs:attribute>
                <xs:attribute name="IsRoot" type="xs:boolean" default="false"/>
                <xs:attribute name="IsChildrenInherited" type="xs:boolean" default="true"/>
                <xs:attribute name="IconId" type="types:Id"/>
                <xs:attribute name="LogoId" type="types:Id"/>
                <xs:attribute name="IsHidden" type="xs:boolean" use="optional" default="false"/>
                <xs:attribute name="UseCustomView" type="xs:boolean"/>
                <xs:attribute name="ClientEnvironment">
                    <xs:simpleType>
                        <xs:annotation>
                            <xs:appinfo source="http://schemas.radixware.org/types.xsd">
                                <types:class>org.radixware.kernel.common.enums.ERuntimeEnvironmentType</types:class>
                            </xs:appinfo>
                        </xs:annotation>
                        <xs:restriction base="types:StrEnum"/>
                    </xs:simpleType>
                </xs:attribute>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="EditorPage">
        <xs:complexContent>
            <xs:extension base="this:TitledAdsDefinition">
                <xs:sequence>
                    <xs:element name="View" type="this:AbstractDialogDefinition" minOccurs="0"/>
                    <xs:element name="WebView" type="this:AbstractDialogDefinition" minOccurs="0"/>
                    <xs:element name="Properties">
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="Property" type="this:EditorPageItem" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
                <xs:attribute name="Type" use="required">
                    <xs:simpleType>
                        <xs:annotation>
                            <xs:appinfo source="http://schemas.radixware.org/types.xsd">
                                <types:class>org.radixware.kernel.common.enums.EEditorPageType</types:class>
                            </xs:appinfo>
                        </xs:annotation>
                        <xs:restriction base="types:IntEnum"/>
                    </xs:simpleType>
                </xs:attribute>
                <xs:attribute name="IconId" type="types:Id"/>
                <xs:attribute name="IsVisible" type="xs:boolean" use="optional" default="true"/>
                <xs:attribute name="CustomEditorId" type="types:Id"/>
                <xs:attribute name="EmbeddedExplorerItemId" type="types:Id"/>
                <xs:attribute name="ClientEnvironment">
                    <xs:simpleType>
                        <xs:annotation>
                            <xs:appinfo source="http://schemas.radixware.org/types.xsd">
                                <types:class>org.radixware.kernel.common.enums.ERuntimeEnvironmentType</types:class>
                            </xs:appinfo>
                        </xs:annotation>
                        <xs:restriction base="types:StrEnum"/>
                    </xs:simpleType>
                </xs:attribute>
                <xs:attribute name="SuppressedWarnings" type="utils:SuppressedWarnings" use="optional"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="EditorPages">
        <xs:sequence>
            <xs:element name="Page" type="this:EditorPage" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="PageOrder">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="Item" maxOccurs="unbounded">
                            <xs:complexType>
                                <xs:attribute name="PageId" type="types:Id" use="required"/>
                                <xs:attribute name="Level" type="xs:int" use="required"/>
                            </xs:complexType>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:annotation>
        <xs:documentation>--------- Class and Class Components  ---------</xs:documentation>
    </xs:annotation>
    <xs:complexType name="ParameterDeclaration">
        <xs:annotation>
            <xs:documentation> method's parameter</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="Type" type="xscml:TypeDeclaration"/>
            <xs:element name="Description" type="xs:string"/>
        </xs:sequence>
        <xs:attribute name="Name" type="xs:string" use="required"/>
        <xs:attribute name="Variable" type="xs:boolean" use="optional"/>
        <xs:attribute name="DescriptionId" type="types:Id" use="optional"/>
        <xs:attribute name="Id" type="types:Id" use="required"/>
        <xs:attribute name="isDescriptionInherited" type="xs:boolean"/>
    </xs:complexType>
    <xs:complexType name="AbstractMethodDefinition">
        <xs:complexContent>
            <xs:extension base="this:DescribedAdsDefinition">
                <xs:sequence>
                    <xs:element name="Parameters" minOccurs="0">
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="Parameter" type="this:ParameterDeclaration" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                    <xs:element name="ReturnType" minOccurs="0">
                        <xs:complexType>
                            <xs:complexContent>
                                <xs:extension base="xscml:TypeDeclaration">
                                    <xs:sequence>
                                        <xs:element name="Description" type="xs:string"/>
                                    </xs:sequence>
                                    <xs:attribute name="DescriptionId" type="types:Id" use="optional"/>
                                    <xs:attribute name="isDescriptionInherited" type="xs:boolean"/>
                                </xs:extension>
                            </xs:complexContent>
                        </xs:complexType>
                    </xs:element>
                    <xs:element name="ThrownExceptions" minOccurs="0">
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="Exception" minOccurs="0" maxOccurs="unbounded">
                                    <xs:complexType>
                                        <xs:complexContent>
                                            <xs:extension base="xscml:TypeDeclaration">
                                                <xs:sequence>
                                                    <xs:element name="Description" type="xs:string"/>
                                                </xs:sequence>
                                                <xs:attribute name="DescriptionId" type="types:Id" use="optional"/>
                                                <xs:attribute name="isDescriptionInherited" type="xs:boolean"/>
                                            </xs:extension>
                                        </xs:complexContent>
                                    </xs:complexType>
                                </xs:element>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
                <xs:attribute name="IsReflectiveCallable" type="xs:boolean" use="optional" default="false"/>
                <xs:attribute name="Nature" use="required">
                    <xs:simpleType>
                        <xs:annotation>
                            <xs:appinfo source="http://schemas.radixware.org/types.xsd">
                                <types:class>org.radixware.kernel.common.enums.EMethodNature</types:class>
                            </xs:appinfo>
                        </xs:annotation>
                        <xs:restriction base="types:IntEnum"/>
                    </xs:simpleType>
                </xs:attribute>
                <xs:attribute name="IsConstructor" type="xs:boolean" use="optional" default="false"/>
                <xs:attribute name="rpcCallId" type="types:Id" use="optional"/>
                <xs:attribute name="Environment">
                    <xs:simpleType>
                        <xs:annotation>
                            <xs:appinfo source="http://schemas.radixware.org/types.xsd">
                                <types:class>org.radixware.kernel.common.enums.ERuntimeEnvironmentType</types:class>
                            </xs:appinfo>
                        </xs:annotation>
                        <xs:restriction base="types:StrEnum"/>
                    </xs:simpleType>
                </xs:attribute>
                <xs:attribute name="isDescriptionInherited" type="xs:boolean" default="false"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="MethodDefinition">
        <xs:complexContent>
            <xs:extension base="this:AbstractMethodDefinition">
                <xs:sequence>
                    <xs:element name="ProfileInfo" type="common:ProfileInfo" minOccurs="0"/>
                    <xs:element name="Source" type="xscml:JmlType" minOccurs="0"/>
                    <xs:element name="Sources" type="this:ClassSource" minOccurs="0"/>
                </xs:sequence>
                <xs:attribute name="IsOverride" type="xs:boolean" use="optional" default="false"/>
                <xs:attribute name="SuppressedWarnings" type="utils:SuppressedWarnings" use="optional"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="PropertyPresentation">
        <xs:complexContent>
            <xs:extension base="common:Definition">
                <xs:sequence>
                    <xs:element name="Editing">
                        <xs:complexType>
                            <xs:complexContent>
                                <xs:extension base="this:EditOptions">
                                    <xs:attribute name="EditPossibility" type="xs:long" use="optional"/>
                                    <xs:attribute name="ReadSeparately" type="xs:boolean" use="optional" default="false"/>
                                    <xs:attribute name="IsDuplicatesEnabled" type="xs:boolean" use="optional" default="true"/>
                                    <xs:attribute name="ObjectEditorPresentationId" type="types:Id"/>
                                    <xs:attribute name="IsArrayElementMandatory" type="xs:boolean" use="optional" default="false"/>
                                    <xs:attribute name="NullArrayTitleId" type="types:Id"/>
                                    <xs:attribute name="ArrayMinSize" type="xs:int" use="optional"/>
                                    <xs:attribute name="ArrayMaxSize" type="xs:int" use="optional"/>
                                    <xs:attribute name="ArrayBaseIndex" type="xs:int" use="optional"/>
                                    
                                    <xs:attribute name="ObjectEditorPresentationIdList" use="optional">
                                        <xs:simpleType>
                                            <xs:list itemType="types:Id"/>
                                        </xs:simpleType>
                                    </xs:attribute>
                                </xs:extension>
                            </xs:complexContent>
                        </xs:complexType>
                    </xs:element>
                    <xs:element name="ParentTitle" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>for ParentRef, ArrRef</xs:documentation>
                        </xs:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="TitleFormat" type="this:ObjectTitleFormatDefinition" minOccurs="0"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                    <xs:element name="Object" minOccurs="0">
                        <xs:complexType>
                            <xs:attribute name="AutoSortClasses" type="xs:boolean"/>
                            <xs:attribute name="ObjectClassCatalogId" type="xs:string"/>
                            <xs:attribute name="ObjectCreationPresentationId" type="xs:string">
                                <xs:annotation>
                                    <xs:appinfo source="http://schemas.radixware.org/types.xsd">
                                        <types:deprecated/>
                                    </xs:appinfo>
                                </xs:annotation>
                            </xs:attribute>
                            <xs:attribute name="CreationEditorPresentationIds" use="optional">
                                <xs:simpleType>
                                    <xs:list itemType="types:Id"/>
                                </xs:simpleType>
                            </xs:attribute>
                        </xs:complexType>
                    </xs:element>
                    <xs:element name="ParentSelect" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>for ParentRef, ArrRef</xs:documentation>
                        </xs:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="ParentSelectCondition" type="this:SqmlCondition" minOccurs="0">
                                    <xs:annotation>
                                        <xs:documentation>for ParentRef, ArrRef</xs:documentation>
                                    </xs:annotation>
                                </xs:element>
                            </xs:sequence>
                            <xs:attribute name="ParentSelectorPresentationId" type="xs:string"/>
                            <xs:attribute name="ParentSelectorRestrictions" type="xs:long" use="optional"/>
                            <xs:attribute name="ParentEditorRestrictions" type="xs:long" use="optional"/>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
                <xs:attribute name="TitleId" type="types:Id"/>
                <xs:attribute name="HintId" type="types:Id"/>
                <xs:attribute name="IsPresentable" type="xs:boolean" use="optional" default="true"/>
                <xs:attribute name="InheritanceMask" type="xs:long" default="0">
                    <xs:annotation>
                        <xs:documentation>
                            Mask for attribute inheritance:  CPropAttrInheritanceMask:
                            - Titles
                            - Editing
                            - ParentTitle
                            - ParentSelector (ParentSelectorPresentationId)
                            - ParentSelectCondition
                            Used only in overriding properties
                        </xs:documentation>
                    </xs:annotation>
                </xs:attribute>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="AdsValAsStr">
        <xs:choice>
            <xs:element name="ValAsStr" type="xs:string"/>
            <xs:element name="Jml" type="xscml:JmlType"/>
        </xs:choice>
    </xs:complexType>
    <xs:complexType name="AbstractPropertyDefinition">
        <xs:complexContent>
            <xs:extension base="this:DescribedAdsDefinition">
                <xs:sequence>
