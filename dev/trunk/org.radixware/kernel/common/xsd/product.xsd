<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" attributeFormDefault="unqualified" elementFormDefault="qualified" targetNamespace="http://schemas.radixware.org/product.xsd"
           xmlns:this="http://schemas.radixware.org/product.xsd" xmlns:types="http://schemas.radixware.org/types.xsd" xmlns:utils="http://schemas.radixware.org/utils.xsd">

    <xs:import namespace="http://schemas.radixware.org/types.xsd" schemaLocation="types.xsd"/>
    <xs:import namespace="http://schemas.radixware.org/utils.xsd" schemaLocation="utils.xsd"/>

    <xs:annotation>
        <xs:documentation>----- Common Types -----</xs:documentation>
    </xs:annotation>


 
 
    <xs:complexType name="DatabaseVersion">        
        <xs:attribute name="DatabaseUrl" type="types:Str" use="required"/>
        <xs:attribute name="SchemeName" type="types:Str" use="required"/>
        <xs:attribute name="ReleaseNumber" type="types:Str" use="required"/>
        <xs:attribute name="Revision" type="xs:long" use="optional"/>
    </xs:complexType>    
 
 
    <xs:complexType name="AboutLayer">
        <xs:all>
            <xs:element name="DbSegment" minOccurs="1" maxOccurs="1">
                <xs:complexType>
                    <xs:sequence minOccurs="0" maxOccurs="unbounded">
                        <xs:element name="DatabaseVersion" type="this:DatabaseVersion"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>            
            
            <xs:element name="KernelSegment" minOccurs="1" maxOccurs="1">
                <xs:complexType>
                    <xs:attribute name="ReleaseNumber" type="types:Str" use="required"/>
                    <xs:attribute name="Revision" type="xs:long" use="optional"/>
                    <xs:attribute name="PrevCompatibleRevisions">
                        <xs:simpleType>
                            <xs:list itemType="types:Str"/>
                        </xs:simpleType>
                    </xs:attribute>                       
                </xs:complexType>
            </xs:element>
            
            <xs:element name="AppSegment" minOccurs="1" maxOccurs="1">
                <xs:complexType>
                    <xs:attribute name="ReleaseNumber" type="types:Str" use="required"/>
                    <xs:attribute name="Revision" type="xs:long" use="optional"/>
                </xs:complexType>
            </xs:element>
                        
        </xs:all>
        <xs:attribute name="Uri" type="types:Str" use="required"/> 
    </xs:complexType>
    
    <xs:complexType name="AadcMember">
        <xs:attribute name="StickToRevision" type="xs:long" use="optional"/>
    </xs:complexType>
  
     <xs:complexType name="AboutBranch">
         <xs:sequence minOccurs="0">
            <xs:element name="AadcMember1" type="this:AadcMember" minOccurs="0" maxOccurs="1"/>
            <xs:element name="AadcMember2" type="this:AadcMember" minOccurs="0" maxOccurs="1"/>
            <xs:element name="Layers">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element minOccurs="0"  maxOccurs="unbounded" name="Layer" type="this:AboutLayer"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>



    <xs:complexType name="DistributiveId">
        <xs:attribute name="ReleaseNumber" type="types:Str" use="required"/>
        <xs:attribute name="DistribVariant" type="types:Int"/>
    </xs:complexType>

    <xs:simpleType name="DistributiveGiveEnum">
        <xs:restriction base="xs:string">
            <xs:enumeration value="None"/>
            <xs:enumeration value="Bin"/>
            <xs:enumeration value="BinAndSrc"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:complexType name="NotifyParameters">
        <xs:sequence>
            <xs:element maxOccurs="unbounded" name="Parameter" type="types:Str"/>
        </xs:sequence>
    </xs:complexType>
    
    <xs:complexType name="NotifySubscribers">
        <xs:sequence>
            <xs:element maxOccurs="unbounded" minOccurs="0" name="Subscriber">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element minOccurs="0" name="Parameters" type="this:NotifyParameters"/>
                    </xs:sequence>
                    <xs:attribute name="Email" type="types:Str" use="required"/>
                    <xs:attribute name="Language" use="optional">
                        <xs:simpleType>
                            <xs:annotation>
                                <xs:appinfo source="http://schemas.radixware.org/types.xsd">
                                    <types:class>org.radixware.kernel.common.enums.EIsoLanguage</types:class>
                                </xs:appinfo>
                            </xs:annotation>
                            <xs:restriction base="types:StrEnum"/>
                        </xs:simpleType>
                    </xs:attribute>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="Signature">
        <xs:simpleContent>
            <xs:extension base="xs:hexBinary">
                <xs:attribute name="Algorithm" type="types:Str" use="required"/>
            </xs:extension>
        </xs:simpleContent>
    </xs:complexType>

    <xs:complexType name="DatabaseOptionDependency">
        <xs:attribute name="Option" type="types:Str" use="required"/>
        <xs:attribute name="Mode" use="optional">
            <xs:simpleType>
                <xs:annotation>
                    <xs:appinfo source="http://schemas.radixware.org/types.xsd">
                        <types:class>org.radixware.kernel.common.enums.EOptionMode</types:class>
                    </xs:appinfo>
                </xs:annotation>
                <xs:restriction base="types:StrEnum"/>
            </xs:simpleType>
        </xs:attribute>
        <xs:attribute name="DefaultMode" use="optional">
            <xs:simpleType>
                <xs:annotation>
                    <xs:appinfo source="http://schemas.radixware.org/types.xsd">
                        <types:class>org.radixware.kernel.common.enums.EOptionMode</types:class>
                    </xs:appinfo>
                </xs:annotation>
                <xs:restriction base="types:StrEnum"/>
            </xs:simpleType>
        </xs:attribute>
        <xs:attribute name="Editable" type="types:Bool" use="optional"/>
        <xs:attribute name="DefaultUpgradeMode" use="optional">
            <xs:simpleType>
                <xs:annotation>
                    <xs:appinfo source="http://schemas.radixware.org/types.xsd">
                        <types:class>org.radixware.kernel.common.enums.EOptionMode</types:class>
                    </xs:appinfo>
                </xs:annotation>
                <xs:restriction base="types:StrEnum"/>
            </xs:simpleType>
        </xs:attribute>
        <xs:attribute name="EditableOnUpgrade" type="types:Bool" use="optional"/>
    </xs:complexType>

    <xs:complexType name="TargetDatabase">
        <xs:all>
            <xs:element name="Options" minOccurs="1" maxOccurs="1">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="Option" minOccurs="0" maxOccurs="unbounded">
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name="Dependency" type="this:DatabaseOptionDependency" minOccurs="0" maxOccurs="unbounded"/>
                                </xs:sequence>
                                <xs:attribute name="Name" type="types:Str" use="required"/>
                                <xs:attribute name="Description" type="types:Str" use="optional"/>
                                <xs:attribute name="DefaultMode" use="required">
                                    <xs:simpleType>
                                        <xs:annotation>
                                            <xs:appinfo source="http://schemas.radixware.org/types.xsd">
                                                <types:class>org.radixware.kernel.common.enums.EOptionMode</types:class>
                                            </xs:appinfo>
                                        </xs:annotation>
                                        <xs:restriction base="types:StrEnum"/>
                                    </xs:simpleType>
                                </xs:attribute>
                                <xs:attribute name="DefaultUpgradeMode" use="required">
                                    <xs:simpleType>
                                        <xs:annotation>
                                            <xs:appinfo source="http://schemas.radixware.org/types.xsd">
                                                <types:class>org.radixware.kernel.common.enums.EOptionMode</types:class>
                                            </xs:appinfo>
                                        </xs:annotation>
                                        <xs:restriction base="types:StrEnum"/>
                                    </xs:simpleType>
                                </xs:attribute>
                                <xs:attribute name="EditableOnUpgrade" type="types:Bool" use="required"/>
                            </xs:complexType>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="Dependencies" minOccurs="1" maxOccurs="1">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="Dependency" type="this:DatabaseOptionDependency" minOccurs="0" maxOccurs="unbounded"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
        </xs:all>
        <xs:attribute name="Type" type="types:Str" use="required"/>
        <xs:attribute name="SupportedVersions" type="types:Str" use="required"/>
    </xs:complexType>
    
    <xs:complexType name="TargetDatabases">
        <xs:sequence>
            <xs:element maxOccurs="unbounded" minOccurs="0" name="TargetDatabase" type="this:TargetDatabase"/>
        </xs:sequence>
    </xs:complexType>


    <xs:complexType name="License">
        <xs:sequence>
            <xs:element maxOccurs="unbounded" minOccurs="0" name="License" type="this:License"/>
            <xs:element name="RequiredModules" minOccurs="0">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="RequiredModule" minOccurs="0" maxOccurs="unbounded">
                            <xs:complexType>
                                <xs:attribute name="Id" type="types:Id"/>
                                <xs:attribute name="LayerUri" type="xs:string"/>
                            </xs:complexType>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
        <xs:attribute name="Name" type="types:Str" use="required"/>
        <xs:attribute name="Dependencies" type="types:Str" use="optional"/>
    </xs:complexType>

    <xs:complexType name="Licenses">
        <xs:sequence>
            <xs:element maxOccurs="unbounded" minOccurs="0" name="License" type="this:License"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="Domain">
        <xs:sequence>
            <xs:element minOccurs="0" name="Definitions">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element maxOccurs="unbounded" minOccurs="0" name="Definition">
                            <xs:complexType>
                                <xs:attribute name="Id" type="types:Str" use="required"/>
                                <xs:attribute name="SubId" type="types:Str"/>
                            </xs:complexType>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element minOccurs="0" name="ChildDomains" type="this:Domains"/>
        </xs:sequence>
        <xs:attribute name="Id" type="types:Str" use="required"/>
        <xs:attribute name="Name" type="types:Str" use="required"/>
    </xs:complexType>

    <xs:complexType name="FileChanges">
        <xs:sequence>
            <xs:element maxOccurs="unbounded" minOccurs="0" name="Add">
                <xs:complexType>
                    <xs:attribute name="Name" type="types:Str" use="required"/>
                </xs:complexType>
            </xs:element>
            <xs:element maxOccurs="unbounded" minOccurs="0" name="Modify">
                <xs:complexType>
                    <xs:attribute name="Name" type="types:Str" use="required"/>
                </xs:complexType>
            </xs:element>
            <xs:element maxOccurs="unbounded" minOccurs="0" name="Remove">
                <xs:complexType>
                    <xs:attribute name="Name" type="types:Str" use="required"/>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:annotation>
        <xs:documentation>----- Configuration Files -----</xs:documentation>
    </xs:annotation>

    <xs:complexType mixed="false" name="NotificationConfig">
        <xs:sequence>
            <xs:element maxOccurs="unbounded" name="Event">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element minOccurs="0" name="SubjectTemplate" type="types:Str"/>
                        <xs:element minOccurs="0" name="BodyTemplate" type="types:Str"/>


                        <xs:element minOccurs="0" name="LanguageLetterTemplates" type="this:LanguageLetterTemplates"/>


                        <xs:element minOccurs="0" name="Subscribers" type="this:NotifySubscribers"/>
                    </xs:sequence>
                    <xs:attribute name="EventType" use="required">
                        <xs:simpleType>
                            <xs:annotation>
                                <xs:appinfo source="http://schemas.radixware.org/types.xsd">
                                    <types:class>org.radixware.kernel.common.enums.ENotificationEventType</types:class>
                                </xs:appinfo>
                            </xs:annotation>
                            <xs:restriction base="types:StrEnum"/>
                        </xs:simpleType>
                    </xs:attribute>
                </xs:complexType>
            </xs:element>
        </xs:sequence>

        <xs:attribute name="PrefixOfIssueAddress"  type="types:Str" use="optional"/>
        <xs:attribute name="Languages" use="optional">
            <xs:simpleType>
                <xs:list>
                    <xs:simpleType>
                        <xs:annotation>
                            <xs:appinfo source="http://schemas.radixware.org/types.xsd">
                                <types:class>org.radixware.kernel.common.enums.EIsoLanguage</types:class>
                            </xs:appinfo>
                        </xs:annotation>
                        <xs:restriction base="types:StrEnum"/>
                    </xs:simpleType>
                </xs:list>
            </xs:simpleType>
        </xs:attribute>

    </xs:complexType>


    <xs:complexType  name="LanguageLetterTemplates">
        <xs:sequence  minOccurs="0" maxOccurs="unbounded" >
            <xs:element minOccurs="0" name="LanguageLetter" type="this:LanguageLetter"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="LanguageLetter">


        <xs:sequence minOccurs="0" maxOccurs="1" >
            <xs:element name="SubjectTemplate"  type="types:Str"/>
            <xs:element name="BodyTemplate" type="types:Str" />
        </xs:sequence>


        <xs:attribute name="Language" use="optional">
            <xs:simpleType>
                <xs:annotation>
                    <xs:appinfo source="http://schemas.radixware.org/types.xsd">
                        <types:class>org.radixware.kernel.common.enums.EIsoLanguage</types:class>
                    </xs:appinfo>
                </xs:annotation>
                <xs:restriction base="types:StrEnum"/>
            </xs:simpleType>
        </xs:attribute>


    </xs:complexType>


    <xs:complexType name="RepositoryConfig">
        <xs:sequence minOccurs="0">
            <xs:any namespace="##local"/>
            <xs:element name="BinaryCompatibilityOptions" type="this:BinaryCompatibilityOptions"/>              
        </xs:sequence>
       
        
        
        <xs:attribute name="Title" type="types:Str" use="required"/>
        <xs:attribute name="InstallUri" type="types:Str" use="optional"/>
        <xs:attribute name="BaseDevUri" type="types:Str" use="optional">
            <xs:annotation>
                <xs:documentation>base (bottom) layer for development</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="RunUri" type="types:Str" use="optional"/>
 
         
        <xs:attribute name="RunUris">
            <xs:simpleType>
                <xs:list itemType="types:Str"/>
            </xs:simpleType>
        </xs:attribute>   
        
            
        <xs:attribute name="DowngradesOffshootsScriptCheck" use="optional">
            <xs:simpleType>
                <xs:restriction base="xs:string">
                    <xs:enumeration value="None"/>
                    <xs:enumeration value="All"/>
                    <xs:enumeration value="One"/>                                            
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>        
            

        <xs:attribute name="VersionFormat" type="xs:long" use="optional"/>
    </xs:complexType>
    

    <xs:complexType name="BinaryCompatibilityOptions">
        <xs:sequence  minOccurs="0" maxOccurs="1">
            <xs:element name="IgnorePackages">
                <xs:complexType>
                    <xs:sequence minOccurs="0" maxOccurs="unbounded" >
                        <xs:element name="Package">  
                            <xs:complexType>
                                <xs:attribute name="Value" type="types:Str" use="required"/>       
                            </xs:complexType>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType> 


    <xs:complexType name="ReplicationConfig">
        <xs:sequence maxOccurs="unbounded" minOccurs="0">
            <xs:element maxOccurs="unbounded" name="Replica">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="SVNAuthentication">
                            <xs:complexType>
                                <xs:attribute name="Type">
                                    <xs:simpleType>
                                        <xs:annotation>
                                            <xs:appinfo source="http://schemas.radixware.org/types.xsd">
                                                <types:class>org.radixware.kernel.common.enums.ESvnAuthType</types:class>
                                            </xs:appinfo>
                                        </xs:annotation>
                                        <xs:restriction base="types:StrEnum"/>
                                    </xs:simpleType>
                                </xs:attribute>
                                <xs:attribute name="UserName" type="xs:string"/>
                                <xs:attribute name="SSHKeyFile" type="xs:string"/>
                            </xs:complexType>
                        </xs:element>
                    </xs:sequence>
                    <xs:attribute name="Title" type="types:Str" use="required"/>
                    <xs:attribute name="SvnHomeUrl" type="types:Str" use="required"/>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="Databases">
        <xs:sequence maxOccurs="unbounded" minOccurs="0">
            <xs:element maxOccurs="unbounded" name="Database">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element minOccurs="0" name="Parameters">
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element maxOccurs="unbounded" minOccurs="0" name="Parameter">
                                        <xs:complexType>
                                            <xs:sequence>
                                                <xs:element name="Title" type="types:Str"/>
                                                <xs:element name="Value" type="types:Str"/>
                                            </xs:sequence>
                                            <xs:attribute name="Name" type="types:Str" use="required"/>
                                        </xs:complexType>
                                    </xs:element>
                                </xs:sequence>
                            </xs:complexType>
                        </xs:element>
                    </xs:sequence>
                    <xs:attribute name="Uri" type="types:Str" use="required"/>
                    <xs:attribute name="Schema" type="types:Str" use="required"/>
                    <xs:attribute name="Test" type="types:Bool" use="required"/>
                    <xs:attribute name="Name" type="xs:string"/>
                    <xs:attribute name="AadcName" type="types:Str" use="optional"/>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:annotation>
        <xs:documentation>---- Service Files -----</xs:documentation>
    </xs:annotation>

    <xs:complexType name="Branch">
        <xs:sequence minOccurs="0">
            <xs:element minOccurs="0" name="ReleaseMessageLayerGroups">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element maxOccurs="unbounded" minOccurs="0" name="Group">
                            <xs:simpleType>
                                <xs:list itemType="xs:string"/>
                            </xs:simpleType>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <!--xs:any namespace="##any" minOccurs="0" maxOccurs="unbounded"/-->
        </xs:sequence>
        <xs:attribute name="BaseDistUri">
            <xs:simpleType>
                <xs:list itemType="xs:string"/>
            </xs:simpleType>
        </xs:attribute>
        <xs:attribute name="Type" use="required">
            <xs:simpleType>
                <xs:annotation>
                    <xs:appinfo source="http://schemas.radixware.org/types.xsd">
                        <types:class>org.radixware.kernel.common.enums.ERepositoryBranchType</types:class>
                    </xs:appinfo>
                </xs:annotation>
                <xs:restriction base="types:StrEnum"/>
            </xs:simpleType>
        </xs:attribute>
        <xs:attribute name="Name" type="types:Str" use="required"/>
        <xs:attribute name="Title" type="types:Str" use="optional"/>
        <xs:attribute name="BaseDevUri" type="types:Str"/>
        <xs:attribute name="LastRelease" type="types:Str"  use="optional"/>
        <xs:attribute name="SrcBranch" type="types:Str"/>
        <xs:attribute name="BaseRelease" type="types:Str"/>
        <xs:attribute name="FormatVersion" type="xs:int" use="optional"/>
        <xs:anyAttribute namespace="##any"/>
        <!--xs:attribute name="PrevReleases">
            <xs:simpleType>
                <xs:list itemType="types:Str"/>
            </xs:simpleType>
        </xs:attribute-->
    </xs:complexType>

    <xs:complexType name="InspectorBranch">
        <xs:sequence>
            <xs:element maxOccurs="unbounded" name="Layer">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="Path" type="xs:string"/>
                    </xs:sequence>
                    <xs:attribute default="false" name="IsReadOnly" type="xs:boolean" use="optional"/>
                    <xs:attribute default="false" name="IsRelativePath" type="xs:boolean" use="optional"/>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
        <xs:attribute name="Name" type="xs:string"/>
    </xs:complexType>




  
    <xs:complexType name="AadcTransformColumn">
        <xs:attribute name="TargetId" type="types:Id" use="required"/>
        <xs:attribute name="TargetName" type="types:Str" use="required"/>
        <xs:attribute name="Source" type="types:Str" use="optional"/>
        <xs:attribute name="SourceType" use="required">
            <xs:simpleType>
                <xs:annotation>
                    <xs:appinfo source="http://schemas.radixware.org/types.xsd">
                        <types:class>org.radixware.kernel.common.enums.EAadcTransformColumnSourceType</types:class>
                    </xs:appinfo>
                </xs:annotation>                
                <xs:restriction base="types:StrEnum"/>
            </xs:simpleType>
        </xs:attribute>
    </xs:complexType>
      
    <xs:complexType name="AadcTransformTable">
        <xs:sequence>
            <xs:element  maxOccurs="unbounded" minOccurs="0" type="this:AadcTransformColumn" name="Columns"/>
        </xs:sequence>
        <xs:attribute name="TargetId" type="types:Id" use="required"/>
        <xs:attribute name="TargetName" type="types:Str" use="required"/> 
        <xs:attribute name="SourceName" type="types:Str" use="optional"/>
    </xs:complexType> 
    
   <xs:complexType name="AadcTransform">
        <xs:sequence>
             <xs:element type="this:AadcTransformTable" name="Table"  minOccurs="0" maxOccurs="unbounded" />
             <xs:element name="Warning" type="xs:string"  minOccurs="0" maxOccurs="unbounded" />
        </xs:sequence>
        <xs:attribute name="Required" type="xs:boolean" use="required"/>
        <xs:attribute name="Forbidden" type="xs:boolean" use="required"/> 
    </xs:complexType>


    <xs:complexType name="Scripts">
        <xs:sequence>
            <xs:element maxOccurs="unbounded" minOccurs="0" name="Script">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element minOccurs="0" name="BaseLayers">
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element maxOccurs="unbounded" name="Layer">
                                        <xs:complexType>
                                            <xs:attribute name="Uri" type="types:Str" use="required"/>
                                            <xs:attribute name="ReleaseNumber" type="types:Str"/>
                                            <xs:attribute name="LastReleaseNumber" type="types:Str"/>
                                            <xs:attribute name="LastScriptFileName" type="types:Str"/>
                                        </xs:complexType>
                                    </xs:element>
                                </xs:sequence>
                            </xs:complexType>
                        </xs:element>
                        <xs:element minOccurs="0" maxOccurs="1" name="AadcTransform" type="this:AadcTransform" />
                    </xs:sequence>                    
                    <xs:attribute name="FileName" type="types:Str" use="required"/>
                    <xs:attribute default="false" name="IsBackwardCompatible" type="xs:boolean"/>
                    <xs:attribute default="false" name="IsReverse" type="xs:boolean"/>
                    <xs:attribute name="StraightFileName" type="types:Str"/>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    
    

    <xs:complexType name="AccessibleDefinitions">
        <xs:sequence>
            <xs:element name="Root" maxOccurs="unbounded">
                <xs:complexType>
                    <xs:attribute name="Id" type="types:Id"/>
                    <xs:attribute name="LayerUri" type="xs:string"/>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
        <xs:attribute name="Empty" type="xs:boolean" use="optional"/>
    </xs:complexType>


    <xs:complexType mixed="false" name="Layer">
        <xs:sequence>
            <xs:element minOccurs="0" name="Certificates">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element maxOccurs="unbounded" name="Certificate">
                            <xs:complexType>
                                <xs:simpleContent>
                                    <xs:extension base="xs:string">
                                        <xs:attribute name="Number" type="types:Int"/>
                                    </xs:extension>
                                </xs:simpleContent>
                            </xs:complexType>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element minOccurs="0" name="LicenseQueries">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element maxOccurs="unbounded" name="Query">
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name="SqlQuery" type="types:Str"/>
                                    <xs:element name="Fields">
