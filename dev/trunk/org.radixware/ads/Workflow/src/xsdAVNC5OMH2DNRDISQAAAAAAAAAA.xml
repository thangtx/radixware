	</xs:element>
	<xs:element name="CaptureNRs">
		<xs:complexType>
			<xs:attribute name="N" type="t:Int" use="required"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="ReleaseRq">
		<xs:complexType>
