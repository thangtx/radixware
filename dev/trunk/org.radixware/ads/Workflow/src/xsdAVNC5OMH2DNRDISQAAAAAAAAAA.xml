	</xs:element>
	<xs:element name="GetLinkedObjectPidsRq">
		<xs:complexType>
			<xs:attribute name="ProcessId" type="t:Int" use="required"/>
			<xs:attribute name="Role" type="t:Str"/>
			<xs:attribute name="Idx" type="t:Int"/>
			<xs:attribute name="EntityGuid" type="t:Str"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="GetLinkedObjectPidsRs">
		<xs:complexType>
			<xs:sequence minOccurs="0" maxOccurs="unbounded">
