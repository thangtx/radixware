<?xml version="1.0" encoding="UTF-8"?>
<ads:AdsDefinition FormatVersion="0" xmlns:ads="http://schemas.radixware.org/adsdef.xsd"><ads:AdsXmlSchemeDefinition Id="xsdAVNC5OMH2DNRDISQAAAAAAAAAA" Name="CommandsXsd" TargetNamespace="http://schemas.radixware.org/workflowCommands.xsd" NeedsDoc="false" Environment="common"><ads:Documentation/><ads:LinkedSchemas ActualizeOnSave="true">
