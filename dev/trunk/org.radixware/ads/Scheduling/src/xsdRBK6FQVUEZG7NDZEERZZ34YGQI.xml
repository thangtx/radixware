<?xml version="1.0" encoding="UTF-8"?>
<ads:AdsDefinition FormatVersion="0" xmlns:ads="http://schemas.radixware.org/adsdef.xsd"><ads:AdsXmlSchemeDefinition Id="xsdRBK6FQVUEZG7NDZEERZZ34YGQI" Name="TaskExportXsd" TargetNamespace="http://schemas.radixware.org/task-impExp.xsd" Environment="common"><xs:schema targetNamespace="http://schemas.radixware.org/task-impExp.xsd" elementFormDefault="qualified" attributeFormDefault="unqualified" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:xscml="http://schemas.radixware.org/xscml.xsd" xmlns:rcmn="http://schemas.radixware.org/common.xsd" xmlns:this="http://schemas.radixware.org/task-impExp.xsd" xmlns:types="http://schemas.radixware.org/types.xsd" xmlns:schd="http://schemas.tranzaxis.com/scheduling-impExp.xsd">
	<xs:import namespace="http://schemas.radixware.org/types.xsd" schemaLocation="types.xsd"/>
	<xs:import namespace="http://schemas.radixware.org/common.xsd" schemaLocation="common.xsd"/>
	<xs:import namespace="http://schemas.radixware.org/xscml.xsd" schemaLocation="xscml.xsd"/>
	<xs:import namespace="http://schemas.tranzaxis.com/scheduling-impExp.xsd" schemaLocation="scheduling-impExp.xsd"/>
	<xs:complexType name="Task">
