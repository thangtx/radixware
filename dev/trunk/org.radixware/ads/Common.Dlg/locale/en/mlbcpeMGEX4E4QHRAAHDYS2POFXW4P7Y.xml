      <Value Language="en" NeedsCheck="false">Format description:</Value>
    </String>
    <String Id="mlsEIN66WWMPJELXAV5CMFYJJ5A44" SrcKind="1" Version="0">
      <Value Language="en" NeedsCheck="false">Result:</Value>
    </String>
