<?xml version="1.0"?>
<!DOCTYPE editor-macros PUBLIC "-//NetBeans//DTD Editor Macros settings 1.1//EN" "http://www.netbeans.org/dtds/EditorMacros-1_1.dtd">
<editor-macros>
    <macro name="debug-var">
        <code><![CDATA[select-identifier copy-to-clipboard caret-end-line insert-break "System.err.println(\"" paste-from-clipboard " = \" + " paste-from-clipboard ");"]]></code>
    </macro>
    <macro name="cast-to-String">
        <code><![CDATA[cut-to-clipboard "((String)" paste-from-clipboard ")"]]></code>
    </macro>
    <macro name="quote-word">
        <code><![CDATA[caret-begin-word "\"" caret-end-word "\""]]></code>
    </macro>
</editor-macros>
