        <Sources>
          <Src>
            <xsc:Item>
              <xsc:Java>/*
    * set daytime from "daytime" to "date" for example date is 09 Nov 2009
    * 18:03, daytime is 14 Oct 2007 05:22 result is 09 Nov 2009 05:22
    */
if (date == null)
    return null;
if (daytime == null)
    return date;
Calendar cDate = new java.util.GregorianCalendar();
Calendar cDaytime = new java.util.GregorianCalendar();
cDate.setTime(date);
cDaytime.setTime(daytime);
cDate.set(Calendar.HOUR_OF_DAY, cDaytime.get(Calendar.HOUR_OF_DAY));
cDate.set(Calendar.MINUTE, cDaytime.get(Calendar.MINUTE));
cDate.set(Calendar.SECOND, cDaytime.get(Calendar.SECOND));
cDate.set(Calendar.MILLISECOND, cDaytime.get(Calendar.MILLISECOND));

return new </xsc:Java>
            </xsc:Item>
