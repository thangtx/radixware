        <ReturnType TypeId="401" extStr="long"/>
        <Sources>
          <Src>
            <xsc:Item>
              <xsc:Java>//возвращает количество дней между двумя датами: 0 - этот же день, 1 - вчера и т.д.
		
GregorianCalendar c=new GregorianCalendar();
c.setTime(newDay);
GregorianCalendar cNewDay=new GregorianCalendar(c.get(Calendar.YEAR),c.get(Calendar.MONTH),c.get(Calendar.DAY_OF_MONTH));
c.setTime(oldDay);
GregorianCalendar cOldDay=new GregorianCalendar(c.get(Calendar.YEAR),c.get(Calendar.MONTH),c.get(Calendar.DAY_OF_MONTH));

return Math.round(((double)(cNewDay.getTimeInMillis()-cOldDay.getTimeInMillis()))/MILLIS_IN_DAY);</xsc:Java>
            </xsc:Item>
          </Src>
        </Sources>
