      <Method Id="mthZ3PSDX56CFAZXET3YHYENOPUSY" Name="setDaytimeInMinutes" Nature="0" IsOverride="false">
        <AccessRules IsStatic="true"/>
        <Parameters>
          <Parameter Name="date" Id="mprVHHJWPM3TFHCPC6ZPT2JLY5XYU">
            <Type TypeId="12"/>
          </Parameter>
          <Parameter Name="daytime" Id="mpr2AED55XQHNCHDP5UQ2MW5O3EHI">
            <Type TypeId="12"/>
          </Parameter>
        </Parameters>
        <ReturnType TypeId="12"/>
        <Sources>
          <Src>
            <xsc:Item>
              <xsc:Java>/* set daytime from "daytime" to "date"
 * for example
 * date is 09 Nov 2009 18:03, daytime is 14 Oct 2007 05:22
 * result is 09 Nov 2009 05:22
 */
if (date == null)
    return null;
if (daytime == null)	
    return date;
Calendar cDate = new java.util.GregorianCalendar();
Calendar cDaytime = new java.util.GregorianCalendar();
cDate.setTime(date);
cDaytime.setTime(daytime);
cDate.set(Calendar.HOUR_OF_DAY, cDaytime.get(Calendar.HOUR_OF_DAY));
cDate.set(Calendar.MINUTE, cDaytime.get(Calendar.MINUTE));

return new </xsc:Java>
            </xsc:Item>
            <xsc:Item>
              <xsc:TypeDeclaration TypeId="12"/>
            </xsc:Item>
            <xsc:Item>
              <xsc:Java>(cDate.getTimeInMillis());
</xsc:Java>
            </xsc:Item>
          </Src>
        </Sources>
      </Method>
